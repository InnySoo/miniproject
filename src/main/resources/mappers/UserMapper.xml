<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sooin.miniproject.mappers.UserMapper">

    <select id="findByUserName" parameterType="string" resultType="com.sooin.miniproject.models.User">
        SELECT * FROM Users WHERE UserName = #{userName}
    </select>

</mapper>
